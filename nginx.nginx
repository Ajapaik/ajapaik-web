upstream django {
    server unix:/home/docker/ajapaik/app.sock;
}

server {
    listen 80 default_server;
    server_name localhost;
    charset     utf-8;
    client_max_body_size 75M;
    location /media  {
        alias /home/docker/persistent/media;
    }
    location /static {
        alias /home/docker/volatile/static;
    }
    location / {
        uwsgi_pass django;
        include /home/docker/ajapaik/uwsgi_params;
    }
}